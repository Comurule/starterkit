
<!-- begin:: Content -->
<div class="kt-container--fluid  kt-grid__item kt-grid__item--fluid">
	<!--begin::Portlet-->
	<div class="kt-portlet">
		<div class="kt-portlet__body">
			<form class="kt-form kt-form--label-right" id="kt_form_1" onsubmit="updateContact(event, <%= contact.id %>)">
				<div class="form-group form-group-last kt-hide">
					<div class="alert alert-danger" role="alert" id="kt_form_1_msg">
						<div class="alert-icon"><i class="flaticon-warning"></i></div>
						<div class="alert-text">
							Oh snap! Change a few things up and try submitting again.
						</div>
						<div class="alert-close">
							<button type="button" class="close" data-dismiss="alert" aria-label="Close">
								<span aria-hidden="true"><i class="la la-close"></i></span>
							</button>
						</div>
					</div>
				</div>
				<div class="kt-section kt-section--first">
					<div class="form-group row">
						<div>
							<h3 class="kt-portlet__head-title"><spam><i class="flaticon-avatar"></i></spam>&nbsp;&nbsp;Lead BioData</h3>
						</div>
					</div>
					<div class="form-group row">
						<div class="form-group col-md-6 col-lg-4">
							<label>First Name:</label>
							<input type="text" class="form-control" name="firstName" value="<%= (contact.firstName != '')? contact.firstName : ''%>" placeholder="Enter first name">
						</div>
						<div class="form-group col-md-6 col-lg-4">
							<label>Last Name:</label>
							<input type="text" class="form-control" name="lastName"  value="<%= (contact.lastName != '')? contact.lastName : ''%>" placeholder="Enter last name">
						</div>
						<div class="form-group col-md-6 col-lg-4">
							<label>Username:</label>
							<input type="text" class="form-control" name="username" placeholder="Enter a preferred name" value="<%= (contact.username != '')? contact.username : ''%>">
						</div>
						<div class="form-group col-md-6 col-lg-4">
							<label>Email:</label>
							<input type="text" class="form-control" name="email" value="<%= (contact.email != '')? contact.email : ''%>" placeholder="Enter Contact's Email">
						</div>
						<div class="form-group col-md-6 col-lg-4">
							<label>Contact's Preferred Language:</label>
							<input type="text" class="form-control" name="mailingLanguage" value="<%= (contact.mailingLanguage != '')? contact.mailingLanguage : ''%>" placeholder="Enter a preferred language">
						</div>
						<div class="form-group col-md-9 col-lg-8">
							<label>Contact Address:</label>
							<input type="text" class="form-control" name="address" value="<%= (contact.address != '')? contact.address : ''%>" placeholder="Enter Contact Address">
						</div>
						<div class="form-group col-md-4 col-lg-2">
							<label>State:</label>
							<input type="text" class="form-control" name="city" value="<%= (contact.city != '')? contact.city : ''%>" placeholder="Enter Contact's City">
						</div>
						<div class="form-group col-md-4 col-lg-2">
							<label>Country:</label>
							<input type="text" class="form-control" name="country" value="<%= (contact.country != '')? contact.country : ''%>" placeholder="Enter Contact's Country">
						</div>
					</div>
				</div>
					<div>
						<hr>
					</div>
				<div class="kt-container--fluid">
					<div class="form-group row">
						<div>
							<h3 class="kt-portlet__head-title">
								<spam><i class="flaticon-settings-1"></i></spam>&nbsp;&nbsp;
								Contact Preferences
							</h3>
						</div>
					</div>
					<div class="form-group row">
			
						<div class="form-group col-xl-4 col-lg-4 col-sm-12">
							<label class="">Preferred Currency</label>
							<div class="">
							<!-- <select class="form-control kt-select2" id="kt_select2_4" name="leadCurrency">
								<option></option>
								<optgroup label="Alaskan/Hawaiian Time Zone">
									<option value="AK">Alaska</option>
									<option value="HI">Hawaii</option>
								</optgroup>
							</select> -->
							<input class="form-control" type="text" name="mailingCurrency" value="<%= contact.mailingCurrency %>">
							</div>
						</div>
						<div class="form-group col-xl-6 col-lg-6 col-sm-12">
							<label class="">Preference Centers</label>
							<div>
								<select class="form-control kt-select2" id="kt_select2_3" name="preferences" multiple="multiple">
										<option value="">-------Choose-------</option>
								</select>
							</div>
						</div>
						<div class="col-lg-9 col-md-9 col-sm-12">
							<a href="" class="btn btn-brand kt-btn kt-btn--pill" data-toggle="modal" data-target="#company_modal">Click Here To Fill In Contact's Company Details</a>
						</div>
						
						<!--begin::Modal-->
						<div class="modal fade" id="company_modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
							<div class="modal-dialog modal-lg" role="document">
								<div class="modal-content">
									<div class="modal-header">
										<h5 class="modal-title" id="exampleModalLabel">Company Details</h5>
										<button type="button" class="close" data-dismiss="modal" aria-label="Close">
											<span aria-hidden="true" class="la la-remove"></span>
										</button>
									</div>
									<div class="kt-portlet__body">
										<div class="row">
											<div class="form-group col-12">
												<label>Company Name:</label>
												<input type="text" class="form-control" name="mailingName" value="<%= (contact.mailingName != '')? contact.mailingName : ''%>" placeholder="MicroSolutions.inc">
											</div>
											<div class="form-group col-8">
												<label>Company Address:</label>
												<input type="text" class="form-control" name="mailingAddress" value="<%= (contact.mailingAddress != '')? contact.mailingAddress : ''%>" placeholder="Enter company address">
											</div>
											<div class="form-group col-2">
												<label>State</label>
												<div class="input-group">														
													<input type="text" class="form-control" name="mailingCity" value="<%= (contact.mailingCity != '')? contact.mailingCity : ''%>" placeholder="">
												</div>
											</div>
											<div class="form-group col-2">
												<label>Country</label>
												<div class="input-group">														
													<input type="text" class="form-control" name="mailingCountry" value="<%= (contact.mailingCountry != '')? contact.mailingCountry : ''%>" placeholder="">
												</div>
											</div>
											<div class="form-group col-6">
												<label>Email Address:</label>
												<input type="text" class="form-control" name="mailingEmail" value="<%= (contact.mailingEmail != '')? contact.mailingEmail : ''%>" placeholder="admin@microsolutions.com">
											</div>
											<div class="form-group col-6">
												<label>Company Website</label>
												<div class="input-group">														
													<input type="text" class="form-control" name="mailingWebsite" value="<%= (contact.mailingWebsite != '')? contact.mailingWebsite : ''%>" placeholder="www.microsolutions.com">
												</div>
											</div>
											<div class="form-group">
												<div class="col-lg-9 col-md-9 col-sm-12">
													<a href="" class="btn btn-brand kt-btn kt-btn--pill" data-toggle="modal" data-target="#company_modal">Continue</a>
												</div>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
						<!--end::Modal-->
					</div>
				</div>	
				
				<div class="kt-portlet__foot">
					<div class="kt-form__actions kt-form__actions--right">
						<div class="col kt-align-right">
							<button type="submit" class="btn btn-brand">Submit</button>
							<button type="reset" class="btn btn-secondary">Cancel</button>
						</div>
					</div>
				</div>
			</form>
		</div>
	</div>
	<!--end::Portlet-->
</div>

<!-- end::Scrolltop -->



<!--begin::Global Theme Bundle(used by all pages) -->
<script src="assets/plugins/global/plugins.bundle.js" type="text/javascript"></script>
<script src="assets/js/scripts.bundle.js" type="text/javascript"></script>

<!--end::Global Theme Bundle -->



<!-- begin:: Content -->
						<div class="kt-container  kt-container--fluid  kt-grid__item kt-grid__item--fluid">
							
							<!--begin::Row-->
							<div class="row"> 
								<div class="col-12">
									<div class="col-md-12 col-xl-9">
										<!--begin::Portlet-->
										<div class="col-lg-9 col-md-9 col-sm-12">
											<a href="" class="btn btn-brand kt-btn kt-btn--pill" data-toggle="modal" data-target="#company_modal">Click Here To Fill In Lead's Company Details</a>
										</div>
										
										<!--begin::Modal-->
										<div class="modal fade" id="company_modal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
											<div class="modal-dialog modal-lg" role="document">
												<div class="modal-content">
													<form class="kt-form kt-form--label-right" id="kt_form_1">
														<div class="modal-header">
															<h5 class="modal-title" id="exampleModalLabel">Create Preference Center Form</h5>
															<button type="button" class="close" data-dismiss="modal" aria-label="Close">
																<span aria-hidden="true" class="la la-remove"></span>
															</button>
														</div>
														<div class="modal-body">
															<div class="form-group form-group-last kt-hide">
																<div class="alert alert-danger" role="alert" id="kt_form_1_msg">
																	<div class="alert-icon"><i class="flaticon-warning"></i></div>
																	<div class="alert-text">
																		Oh snap! Change a few things up and try submitting again.
																	</div>
																	<div class="alert-close">
																		<button type="button" class="close" data-dismiss="alert" aria-label="Close">
																			<span aria-hidden="true"><i class="la la-close"></i></span>
																		</button>
																	</div>
																</div>
															</div>
															<div class="row">
																<div class="form-group col-6">
																	<label>Name:</label>
																	<input type="text" class="form-control" name="name" placeholder="Financial Preference">
																</div>
															
																<div class="form-group col-2">
																	<label>Code:</label>
																	<div class="input-group">
																		<div class="input-group-prepend"><span class="input-group-text" id="basic-addon1">pc </span></div>														
																		<input type="text" class="form-control" name="pcCode" placeholder="01">
																	</div>
																</div>
																<div class="form-group col-2">
																	<label>Tier:</label>
																	<div class="input-group">
																		<div class="input-group-prepend"><span class="input-group-text" id="basic-addon1">Tier </span></div>														
																		<input type="text" class="form-control" name="tier" placeholder="1">
																	</div>
																</div>
															</div>
															<div>
																<div class="row">
																	<div class="form-group col-xl-4 col-lg-4 col-sm-12">
																		<label class="">Display Type:</label>
																		<div class="">
																		<select class="form-control kt-select2" name="displayType">
																			<option>Select One Option</option>
																			<option value="Discover And Learn">Discover And Learn</option>
																			<option value="Entity">Entity</option>
																			<option value="Preference Center">Preference Center</option>
																			</select>
																		</div>
																	</div>
																	<div class="form-group col-xl-6 col-lg-6 col-sm-12">
																		<label class="">Parent Preference Center:</label>
																		<div class="">
																			<select class="form-control kt-select2" id="kt_select2_4" name="parentPC">
				          													<% preferences.forEach(preference => { %>
				          														<option value="<%=preference.id%>"><%=preference.name%></option>
				          													<% }) %>
																			</select>
																		</div>
																	</div>
																</div>
															</div>
															<div class="kt-form__actions">
																<button type="submit" class="btn btn-brand">Submit</button>
																<button type="reset" class="btn btn-secondary" data-toggle="modal" data-target="#company_modal">Cancel</button>
															</div>                                                                                                                                            
														</div>
													</form>
												</div>
											</div>
										</div>
										<!--end::Modal-->
										<!--end::Portlet-->
									</div>
								</div>
							</div>
							<!--end::Row-->
						</div>
						<!--for Form select Options -->
						<script src="assets/js/pages/components/forms/widgets/select2.js" type="text/javascript"></script>
						
						<!-- end:: Content -->
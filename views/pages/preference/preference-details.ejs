<!--begin::Modal-->
<div class="modal-dialog modal-lg" role="document">
										<div class="modal-content">
											<form class="kt-form kt-form--label-right" id="kt_form_1" action="/main/preferences/create" method="post">
												<div class="modal-header">
													<h5 class="modal-title" id="exampleModalLabel">Create Preference Center Form</h5>
													<button type="button" class="close" data-dismiss="modal" aria-label="Close">
														<span aria-hidden="true" class="la la-remove"></span>
													</button>
												</div>
												<div class="modal-body">
													<div class="form-group form-group-last kt-hide">
														<div class="alert alert-danger" role="alert" id="kt_form_1_msg">
															<div class="alert-icon"><i class="flaticon-warning"></i></div>
															<div class="alert-text">
															</div>
														</div>
													</div>
													<div class="row">
														<div class="form-group col-6">
															<label>Name:</label>
															<input type="text" class="form-control" name="name" value="<%= preference.name%>" placeholder="Financial Preference">
														</div>
													
														<div class="form-group col-2">
															<label>Code:</label>
															<div class="input-group">
																<div class="input-group-prepend"><span class="input-group-text" id="basic-addon1">pc </span></div>														
																<input type="number" class="form-control" name="pcCode" value="<%= preference.pcCode%>" placeholder="01">
															</div>
														</div>
														<div class="form-group col-2">
															<label>Tier:</label>
															<div class="input-group">
																<div class="input-group-prepend"><span class="input-group-text" id="basic-addon1">Tier </span></div>														
																<input type="number" class="form-control" name="tier" value="<%= preference.tier%>" placeholder="1">
															</div>
														</div>
													</div>
													<div>
														<div class="row">
															<div class="form-group col-xl-4 col-lg-4 col-sm-12">
																<label class="">Display Type:</label>
																<div class="">
																<select class="form-control kt-select2" id="kt_select2_1" name="displayType">
																	<option>Select An Option</option>
																	<option value="Discover And Learn">Discover And Learn</option>
																	<option value="Entity">Entity</option>
																	<option value="Preference Center">Preference Center</option>
																	</select>
																</div>
															</div>
															<div class="form-group col-xl-6 col-lg-6 col-sm-12">
																<label class="">Parent Preference Center:</label>
																<div class="">
																<select class="form-control kt-select2" id="kt_select2_0" name="parentPC">
																	<option>Select An Option</option>
																	<% preferences.forEach(preference => { %>
		          														<option value="<%=preference.id%>"><%=preference.name%></option>
		          													<% }) %>
																</select>
																</div>
															</div>
														</div>
													</div>
													<div class="kt-form__actions">
														<button type="submit" class="btn btn-brand">Submit</button>
														<button type="reset" class="btn btn-secondary" data-toggle="modal" data-target="#company_modal">Cancel</button>
													</div>                                                                                                                                            
												</div>
											</form>
										</div>
									</div>
<!--end::Modal-->
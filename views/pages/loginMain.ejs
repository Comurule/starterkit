<%- contentFor('main-content') %>
<!-- begin:: Page -->
<div class="kt-grid kt-grid--ver kt-grid--root">
    <div class="kt-grid__item   kt-grid__item--fluid kt-grid  kt-grid kt-grid--hor kt-login-v2" id="kt_login_v2">

        <!--begin::Item-->
        <div class="kt-grid__item  kt-grid--hor">

            <!--begin::Heade-->
            <div class="kt-login-v2__head">
                <div class="kt-login-v2__logo">
                    <a href="#">
                    </a>
                </div>
            </div>

            <!--begin::Head-->
        </div>

        <!--end::Item-->

        <!--begin::Item-->
        <div class="kt-grid__item  kt-grid  kt-grid--ver  kt-grid__item--fluid">

            <!--begin::Body-->
            <div class="kt-login-v2__body">

                <!--begin::Wrapper-->
                <div class="kt-login-v2__wrapper">
                    <div class="kt-login-v2__container">
                        <div class="kt-login-v2__title">
                            <div class="kt-login-v2__logo" style="padding-top: 30px;">
                                <a href="#" class="kt-link--brand">
                                    <h2 class="kt-font-brand">Lead Campaign</h2>
                                </a>
                            </div>
                        </div>
                        <!--begin::Separator-->
                        <div class="kt-separator kt-separator--space-lg  kt-separator--border-solid"></div>

                        <!--end::Separator-->

                        <!--begin::Form-->
                        <form class="kt-login-v2__form kt-form form-brand" action="" method="POST" autocomplete="off" id="kt_login_form">
                            <div class="form-group">
                                <input class="form-control" type="text" placeholder="Email" name="email" autocomplete="off">
                            </div>
                            <div class="form-group">
                                <input class="form-control" type="password" placeholder="Password" name="password" autocomplete="off">
                            </div>

                            <!--begin::Action-->
                            <div class="kt-login-v2__actions">
                                <a href="#" class="kt-link kt-link--brand">
                                    Forgot Password ?
                                </a>
                                <button type="submit" class="btn btn-brand btn-elevate btn-pill" id="kt_login_submit">Log In</button>
                            </div>

                            <!--end::Action-->
                        </form>

                        <!--end::Form-->

                        <!--begin::Separator-->
                        <div class="kt-separator kt-separator--space-lg  kt-separator--border-solid"></div>

                        <!--end::Separator-->

                        <!--begin::Options-->
                        
                        <div class="kt-login-v2__options">
                            <h4>
                                Don't have an account?&nbsp;&nbsp;
                                <a class="kt-link kt-font-brand">Sign Up</a>
                            </h4>
                        </div>

                        <!--end::Options-->
                    </div>
                </div>

                <!--end::Wrapper-->

                <!--begin::Image-->
                <div class="kt-login-v2__image">
                    <img src="assets/media/misc/bg_icon.svg" alt="">
                </div>

                <!--begin::Image-->
            </div>

            <!--begin::Body-->
        </div>

        <!--end::Item-->

        <!--begin::Item-->
        <div class="kt-grid__item">
            <div class="kt-login-v2__footer">
                <div class="kt-login-v2__link">
                    <a href="#" class="kt-link kt-font-brand">Privacy</a>
                    <a href="#" class="kt-link kt-font-brand">Legal</a>
                    <a href="#" class="kt-link kt-font-brand">Contact</a>
                </div>
                <div class="kt-login-v2__info">
                    <a href="#" class="kt-link">&copy; 2020 Comurule | Micro Solutions Inc</a>
                </div>
            </div>
        </div>

        <!--end::Item-->
    </div>
</div>
<!--begin::Page Scripts(used by this page) -->
<script src="assets/js/login.js" type="text/javascript"></script>
<script src="assets/js/jquery.js" type="text/javascript"></script>

<!-- end:: Page -->


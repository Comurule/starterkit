<!-- begin:: Subheader -->
<div class="kt-subheader   kt-grid__item" id="kt_subheader">
    <div class="kt-container  kt-container--fluid ">
        <div class="kt-subheader__main">
            <h3 class="kt-subheader__title">Lead Campaign</h3>
            <span class="kt-subheader__separator kt-hidden"></span>
            <div class="kt-subheader__breadcrumbs">
                <a href="/" class="kt-subheader__breadcrumbs-home"><i class="flaticon2-shelter"></i></a>
                <% if((/details/i).test(title) || (/list/i).test(title)) { %>
                <span class="kt-subheader__breadcrumbs-separator"></span>
                <a href="/main/<%=title.split(' ')[0].toLowerCase()%>s" class="kt-subheader__breadcrumbs-link">
                    <%=title.split(' ')[0]%> </a>
                <span class="kt-subheader__breadcrumbs-separator"></span>
                <a class="kt-subheader__breadcrumbs-link">
                  <%=title.split(' ')[1]%></a>
                <% } else { %>
                <span class="kt-subheader__breadcrumbs-separator"></span>
                <a href="/main/<%=title.split(' ')[1].toLowerCase()%>s" class="kt-subheader__breadcrumbs-link">
                    <%=title.split(' ')[1]%> </a>
                <span class="kt-subheader__breadcrumbs-separator"></span>
                <a class="kt-subheader__breadcrumbs-link">
                  <%=title.split(' ')[0]%></a>
                <% } %>
            </div>
        </div>
    </div>
</div>
<!-- end:: Subheader -->